{
  "name": "gira-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "dev:cap": "node scripts/syncnetworkconfig.js && vite dev --host",
    "build:cap": "vite build && npx cap sync",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev --",
    "compile": "vite build && npx cap sync && cd android && ((CALL .\\gradlew build --info) || (./gradlew build --info))"
  },
  "devDependencies": {
