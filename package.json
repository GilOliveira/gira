    "@capacitor/toast": "^5.0.6",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource-variable/roboto-mono": "^5.0.17",
    "@fontsource/inter": "^5.0.16",
    "@fontsource/roboto-mono": "^5.0.16",
