    "@kitql/graphql-codegen": "^0.5.4",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@tailwindcss/forms": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.32",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.0",
